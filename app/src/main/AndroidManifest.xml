        <activity android:name=".ui.HistoryActivity" />
        <activity android:name=".ui.PaketActivity" />
        <activity android:name=".ui.ProfileActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.PilihanActivity" />
        <activity android:name=".ui.BiodataActivity" />
        <activity android:name=".ui.HomaActivity" />
        <activity android:name=".ui.DetailBooking" />
        <activity android:name=".ui.BookingActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>